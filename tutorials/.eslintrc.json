{
  "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "node": true
  },
  "extends": [
      "google"
  ],
  "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
      "ecmaVersion": 5,
      "impliedStrict": true
  },
  "rules": {
      "comma-dangle": "off",
      "guard-for-in": "off",
      "new-cap": "off",
      "no-var": "off",
      "prefer-const": "off",
      "prefer-rest-params": "off",
      "require-jsdoc": "off",
      "no-unused-vars": "warn"
  }
}